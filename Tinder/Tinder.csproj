    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>92f40976-858e-483f-864b-de1f303bc3fb</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
